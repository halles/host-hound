{
  "name": "HostHound",
  "description": "Recursos Humanos m√°s eficientes",
  "keywords": ["framework", "laravel", "lumen", "angularjs", "bower"],
  "license": "MIT",
  "type": "project",
  "require": {
    "laravel/lumen-framework": "5.0.*",
    "vlucas/phpdotenv": "~1.0",
    "facebook/php-sdk-v4": "4.0.*",
    "firebase/php-jwt": "~2.0",
    "guzzlehttp/guzzle": "5.2.0",
    "guzzlehttp/oauth-subscriber": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "phpspec/phpspec": "~2.1"
  },
  "autoload": {
    "psr-4": {
        "App\\": "backend/app/"
    },
    "classmap": [
        "backend/database/"
    ]
  },
  "autoload-dev": {
    "classmap": [
        "backend/tests/"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}

